<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_reels"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.ReelsActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView_reels_reels"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton_reels_reels"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.162" />

    <ImageButton
        android:id="@+id/imageButton_reels_add"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_reels_reels"
        app:layout_constraintEnd_toStartOf="@+id/imageButton_reels_reels"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageButton_reels_home"
        app:layout_constraintTop_toTopOf="@+id/imageButton_reels_reels"
        app:srcCompat="@drawable/add" />

    <ImageButton
        android:id="@+id/imageButton_reels_home"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_reels_add"
        app:layout_constraintEnd_toStartOf="@+id/imageButton_reels_add"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageButton_reels_add"
        app:srcCompat="@drawable/home" />

    <ImageButton
        android:id="@+id/imageButton_reels_profile"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginBottom="16dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageButton_reels_reels"
        app:srcCompat="@drawable/profile" />

    <ImageButton
        android:id="@+id/imageButton_reels_reels"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_reels_profile"
        app:layout_constraintEnd_toStartOf="@+id/imageButton_reels_profile"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageButton_reels_add"
        app:layout_constraintTop_toTopOf="@+id/imageButton_reels_profile"
        app:srcCompat="@drawable/reels_highlighted" />
</androidx.constraintlayout.widget.ConstraintLayout>